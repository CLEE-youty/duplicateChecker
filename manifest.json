{
  "name": "duplicateChecker",
  "manifest_version": 2,
  "version": "1.0",
  "browser_action": {
    "default_title": "duplicateChecker",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["content_script.css"],
      "js": ["content_script.js"]
    }
  ]
}
